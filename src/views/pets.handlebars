<div>
    <h1>Usuarios</h1>
    <table>
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Especie</th>
                <th>Adoptado</th>
            </tr>
        </thead>
        <tbody>
            {{#each pets}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.specie}}</td>
                <td>{{this.adopted}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
